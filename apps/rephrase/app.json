{
  "expo": {
    "name": "Rephrase",
    "slug": "rephrase",
    "version": "1.0.0",
    "main": "index.ts",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourco.rephrase"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourco.rephrase"
    },
    "web": {
      "favicon": "./assets/favicon.ico"
    }
  }
}
